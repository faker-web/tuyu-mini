<!--index.wxml-->
<view>
  <!-- <navigation-bar default-data="{{defaultData}}"></navigation-bar> -->
  <view class="page-section page-section-spacing swiper">
    <swiper
      indicator-dots="{{indicatorDots}}"
      autoplay="{{autoplay}}"
      interval="{{interval}}"
      duration="{{duration}}"
      style="height: 234px;"
    >
      <block wx:for="{{swiper}}" wx:key="_id">
        <swiper-item>
          <image src="{{item.url}}" class="banner-item"/>
        </swiper-item>
      </block>
    </swiper>
  </view>
  <view class="top-section">
    <notification></notification>
    <cardList cardData="{{card}}" bind:scrollBottom="scrollBottom" bind:ocrClick="ocrClick"></cardList>
  </view>
</view>
